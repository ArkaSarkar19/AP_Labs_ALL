class item {
	private int id;
	private String name;
	private float price;
	private int quantity;
	private String category;
	
	public item(int id, String name, float price, int quantity, String category) {
		this.id = id;
		this.name = name;
		this.price = price;
		this.quantity = quantity;
		this.category = category;
	}
}

